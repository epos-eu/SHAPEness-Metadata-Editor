            icon="icons/test.ico"
            thread="main"
            visible="true">
         <run
               class="org.epos.metadata.editor.ui.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.epos.metadata.editor.EPOSid"
            name="SHAPEness">
         <property
               name="appName"
               value="SHAPEness">
         </property>
         <property name="cssTheme" value="org.eclipse.e4.ui.css.theme.e4_default"/>
         <property
               name="windowImages"
               value="icons/mde-16.png,icons/mde-32.png,icons/mde-48.png,icons/mde-128.png,icons/mde-256.png">
         </property>
         <property
               name="startupForegroundColor"
               value="F9F9FF">
         </property>
         <property
               name="startupMessageRect"
               value="7,560,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,580,920,20">
         </property>
         <property
